{
  "name": "barstool-fullstack-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "./build/index.js",
  "_moduleAliases": {
    "@Types": "build/Types/",
    "@Services": "build/Services/",
    "@Repositories": "build/Repositories/",
    "@Models": "build/Models/"
  },
  "scripts": {
    "db:init": "npx mikro-orm migration:create --initial",
