    "@types/ioredis": "^4.26.6",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.4.7",
    "@types/redis": "^2.8.31",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@barstool-dev/types": "github:judegiordano/bartsool-dev-types",
