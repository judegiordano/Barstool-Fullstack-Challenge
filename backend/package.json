    "db:down": "npx mikro-orm migration:down",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "build": "tsc",
    "dev:watch": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "prestart": "npm run build",
    "start": "node . --max-old-space-size=8192"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mikro-orm/cli": "^4.5.7",
    "@types/axios": "^0.14.0",
