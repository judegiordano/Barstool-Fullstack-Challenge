    "fastify-rate-limit": "^5.6.0",
    "fastify-sensible": "^3.1.1",
    "fastify-swagger": "^4.8.3",
    "ioredis": "^4.27.7",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
